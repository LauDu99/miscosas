package com.laura.maven.mavendemo;

import org.junit.Test;

import junit.framework.Assert;

/**
 * Unit test for simple App.
 */

public class AppTest 
{
	//para marcar este metodo como prueba
	@Test
	//es bueno dar nombres largos para saber que hace la prueba
    public void givenNameWhenHelloThenReturnHelloName() {
    	App app = new App();
    	String result = app.Hello("Laura");
    	//para regresar  el resultado se usan afirmaciones
    	Assert.assertNotNull(result);
    }
}
